@using Application.DTO.Response

<MudDialog>
    <TitleContent>
        Products
    </TitleContent>
    <DialogContent>
        <Product ProductIds="ProductIds" CurrentStaff="CurrentStaff" MainPage="MainPage" PageComponents="PageComponents"/>
    </DialogContent>
</MudDialog>

@code {
    [Parameter]
    public IMainPage MainPage { get; set; }

    [Parameter]
    public List<IPageComponent> PageComponents { get; set; }
    
    [Parameter]
    public List<Guid> ProductIds { get; set; }

    [Parameter]
    public GetStaffResponseDTO CurrentStaff { get; set; }
}

    
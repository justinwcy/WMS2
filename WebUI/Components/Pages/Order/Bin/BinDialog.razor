@using Application.DTO.Response

<MudDialog>
    <TitleContent>
        Bins
    </TitleContent>
    <DialogContent>
        <Bin BinIds="BinIds" CurrentStaff="CurrentStaff" MainPage="MainPage" PageComponents="PageComponents"/>
    </DialogContent>
</MudDialog>

@code {
    [Parameter]
    public IMainPage MainPage { get; set; }

    [Parameter]
    public List<IPageComponent> PageComponents { get; set; }
    
    [Parameter]
    public List<Guid> BinIds { get; set; }

    [Parameter]
    public GetStaffResponseDTO CurrentStaff { get; set; }
}

    
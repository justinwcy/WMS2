@using Application.DTO.Response

<MudDialog>
    <TitleContent>
        Inventories
    </TitleContent>
    <DialogContent>
        <Inventory InventoryIds="InventoryIds" CurrentStaff="CurrentStaff" MainPage="MainPage" PageComponents="PageComponents"/>
    </DialogContent>
</MudDialog>

@code {
    [Parameter]
    public IMainPage MainPage { get; set; }

    [Parameter]
    public List<IPageComponent> PageComponents { get; set; }
    
    [Parameter]
    public List<Guid> InventoryIds { get; set; }

    [Parameter]
    public GetStaffResponseDTO CurrentStaff { get; set; }
}

    